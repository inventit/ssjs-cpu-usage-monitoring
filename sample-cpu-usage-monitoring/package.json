{
    "name" : "sample-cpu-usage-monitoring",
    "version" : "1.0.0",
    "description" : "ServiceSync Sample Application",
    "maintainers" : [
        {
            "name" : "Inventit",
            "email" : "egypt@yourinventit.com"
        }
    ],
    "bugs" : {
        "mail" : "egypt@yourinventit.com"
    },
    "dependencies": {
        "moat": ">=1.0.0 <2.0.0"
    },
    "models" : {
        "CpuUsage" : {
            "array" : false,
            "scope" : "device",
            "attributes" : {
                "user" : { "type" : "float" },
                "nice" : { "type" : "float" },
                "system" : { "type" : "float" },
                "idle" : { "type" : "float" },
                "iowait" : { "type" : "float" },
                "timestamp" : {"type" : "int64"}
            }
        }
    }
}
